      <command:verb>Trace</command:verb>
      <command:noun>SANCertificate</command:noun>
      <maml:description>
        <maml:para>Trace Certificates from Hostnames in a Source Certificate</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>Trace Certificates from Subject Alternative Names (SAN) Hostnames in a Source Certificate</maml:para>
    </maml:description>
